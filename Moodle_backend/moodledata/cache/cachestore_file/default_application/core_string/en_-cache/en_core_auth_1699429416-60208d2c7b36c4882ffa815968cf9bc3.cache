on";s:14:"update_onlogin";s:14:"On every login";s:15:"update_onupdate";s:9:"On update";s:31:"user_activatenotsupportusertype";s:67:"auth: ldap user_activate() does not support selected usertype: {$a}";s:30:"user_disablenotsupportusertype";s:68:"auth: ldap user_disable() does not support selected usertype (..yet)";s:8:"username";s:8:"Username";s:13:"username_help";s:91:"Please be aware that some authentication plugins will not allow you to change the username.";}